<UserControl x:Class="ICSharpCode.ILSpy.Controls.CheckBoxBullet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="13" d:DesignWidth="13">
	<Grid>
		<Border Name="outerBorder"
			Width="13" Height="13"
			BorderThickness="1">
			<Border Name="innerBorder"
				Width="9" Height="9"
				BorderThickness="1"/>
		</Border>
		<Path Name="path1" Data="{DynamicResource CheckMarkGeometry}" StrokeThickness="1.5"/>
		<Path Name="path2" Data="{DynamicResource CheckMarkGeometry}" StrokeThickness="0"/>
	</Grid>
</UserControl>